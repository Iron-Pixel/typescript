{
    "private": true,
    "extensionKind": [
        "workspace"
      ],
    "name": "vscode-typescript",
    "displayName": "JavaScript and TypeScript Nightly",
    "icon": "documentation/logo.png",
    "author": "Microsoft Corp.",
    "homepage": "https://www.typescriptlang.org/",
    "version": "4.1.20200814",
    "license": "Apache-2.0",
    "description": "Enables typescript@next to power VS Code's built-in JavaScript and TypeScript support TypeScript is a language for application scale JavaScript development. This is a Fork to get used with vscode it enables .mjs support",
    "keywords": [
        "TypeScript",
        "Microsoft",
        "compiler",
        "language",
        "javascript"
    ],
    "bugs": {
        "url": "https://github.com/Microsoft/TypeScript/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/TypeScript.git"
    },
    "main": "./lib/typescript.js",
    "typings": "./lib/typescript.d.ts",
    "bin": {
        "tsc": "./bin/tsc",
        "tsserver": "./bin/tsserver"
    },
    "engines": {
        "node": ">=4.2.0",
        "vscode": "^1.36.0"
    },
    "devDependencies": {

    },
    "scripts": {
        "start": "node lib/tsc",
        "setup-hooks": "node scripts/link-hooks.js"
    },
    "browser": {
        "fs": false,
        "os": false,
        "path": false,
        "crypto": false,
        "buffer": false,
        "@microsoft/typescript-etw": false,
        "source-map-support": false,
        "inspector": false
    },
    "dependencies": {}
}